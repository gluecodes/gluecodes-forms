{
  "presets": [
    [
      "@babel/preset-env", {
        "targets": {
          "browsers": [
            "last 2 versions",
            "safari >= 10",
            "IE >= 11"
          ]
        },
        "shippedProposals": true
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    ["@babel/plugin-transform-react-jsx", {
      "pragma": "renderer"
    }]
  ]
}
